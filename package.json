{
    "name": "floobits",
    "displayName": "floobits",
    "description": "Collaborative editing tool",
    "version": "0.0.1",
    "publisher": "Floobits",
    "engines": {
        "vscode": "^1.10.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "floobits.joinWorkspace",
        "*"
    ],
    "main": "./out/src/floobits",
    "contributes": {
        "commands": [{
            "command": "floobits.joinWorkspace",
            "title": "Join Workspace"
        }]
    },
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "@types/mocha": "^2.2.32",
        "@types/node": "^6.0.40",
        "async": "1.x",
        "dmp": "1.x",
        "eslint": "3.x",
        "eslint-config-floobits": "*",
        "eslint-plugin-react": "5.x",
        "event-kit": "1.5.x",
        "flukes": ">=0.1.4",
        "fs-plus": "2.x",
        "lodash": "4.x",
        "minimatch": "3.x",
        "mkdirp": "0.5.x",
        "mocha": "^2.3.3",
        "open": "0.x",
        "request": "2.x",
        "typescript": "^2.0.3",
        "vscode": "^1.0.0"
    }
}
